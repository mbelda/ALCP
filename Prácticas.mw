parejas := proc(G)
	local res, i, j;
	res := {};
	for i from 1 to nops(G) do
		for j from 1 to nops(G) do
			if i &lt;&gt; j then
				res := res union {{G[i], G[j]}};
			end if;
		end do;
	end do;
	return res
end proc:

parejash := proc(h, G)
	local res, i, j;
	res := {};
	for j from 1 to nops(G) do
		res := res union {{h, G[j]}};
	end do;	
	return res
end proc:

mcmMonomios := proc(m1, m2, vars)
	local i, res, d1, d2;
	res := 1;
	for i from 1 to nops(vars) do
		d1 := degree(m1, vars[i]);
		d2 := degree(m2, vars[i]);
		if d1 - d2 &gt; 0 then
			res := res*vars[i]^d1;
		else
			res := res*vars[i]^d2;
		end if;
	end do;
	return res
end proc:

myspoly := proc(f, g, vars, orden)
	local ltf, ltg, mcm;
	ltf := LeadingMonomial(f, orden(op(vars)));
	ltg := LeadingMonomial(g, orden(op(vars)));
	mcm := mcmMonomios(ltf, ltg, vars);
	return expand(simplify(mcm*f/ltf - mcm*g/ltg));
end proc:

mydivide := proc(m1, m2, vars)
	local i;
	for i from 1 to nops(vars) do
		if degree(m2, vars[i]) - degree(m1, vars[i]) &lt; 0 then
			return false
		end if;
	end do;
	return true
end proc:

algunoDivide := proc(farray, h, vars, orden)
	local lti, lth, i;
	for i from 1 to nops(farray) do
		lti := LeadingMonomial(farray[i], orden(op(vars)));
		lth := LeadingMonomial(h, orden(op(vars)));
		if mydivide(lti, lth, vars) then
			return true, i
		end if;
	end do;
	return false, 0
end proc:

divisionPols := proc(f, farray, vars, orden)
	local h, a, i, lti, lth, existe, ret,r;
	h := f;
	a := [seq(0, i=1..nops(farray))];
	r := 0;
	while h &lt;&gt; 0 do
		existe, i := algunoDivide(farray, h, vars, orden);
		while existe do
			lti := `*`(Groebner[LeadingTerm](farray[i], orden(op(vars))));
			lth := `*`(Groebner[LeadingTerm](h, orden(op(vars))));
			a[i] := expand(simplify(a[i] + lth/lti));
			h := expand(simplify(h - lth/lti*farray[i]));
			existe, i := algunoDivide(farray, h, vars, orden);
		end do;

		lth := `*`(Groebner[LeadingTerm](h, orden(op(vars))));
		if h &lt;&gt; 0 then
			r:=simplify(r+lth);
			h:=simplify(h-lth);
		fi;
	end do;
	h:= r;
	return a,h
end proc:

print('________________EJEMPLOS________________');
print(`Division: xy^2 - x con y^2 - 1`);
divisionPols(x*y^2-x, [y^2-1], [x,y], plex);
print(`Division: yx^2 + xy^2 + y^2 con y^2 - 1 y xy-1`);
divisionPols(x^2*y+x*y^2+y^2, [y^2-1, x*y-1], [x, y], plex);

baseGroebner := proc(F, variables, orden)
	local G, paresCriticos, Fi, Fj, gi, gj, a, S, h;
	G := F;
	paresCriticos := parejas(G);
	while paresCriticos &lt;&gt; {} do
		Fi, Fj := paresCriticos[1,1], paresCriticos[1,2];
		paresCriticos := paresCriticos minus {paresCriticos[1]};
		#NF es mas o menos el de division de pols
		#h := NF(myspoly(f,g), G);
		a,h := divisionPols(myspoly(Fi,Fj,variables,orden),G,variables,orden);
		if h &lt;&gt; 0 then 
			paresCriticos := paresCriticos union parejash(h,G);
			G := G union {h};
		end if;
	end do;
	return G;
end proc:

print('________________EJEMPLOS________________');
baseGroebner({x^3, -y^4+x^2*y},[x,y],plex);
print(`Comprobamos:`);
Groebner[Basis]({x^3, -y^4+x^2*y}, plex(x, y));

baseGroebner({x^2, x*y+y^2},[x,y],plex);
print(`Comprobamos:`);
Groebner[Basis]({x^2, x*y+y^2}, plex(x, y));</EC-CodeEditor>
