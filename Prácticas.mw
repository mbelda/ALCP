local result, h, i, g, f, q;
    q := p^expo;
    f := pol;
    result := [];
    h := x;
    i := 1;
    while f &lt;&gt; 1 do
        h := Powmod(h, q, f, x) mod p;
        g := Gcd(h-x, f) mod p;
        if g &lt;&gt; 1 then         
            f := Quo(f, g, x) mod p;
            h := Rem(h, f, x) mod p;
            result := [op(result), g];
        end if;
        i := i+1;
    end do;
    return result
end proc:

