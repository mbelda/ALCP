chinoRestos([49,-21,-30],[99,97,95]);
print(`Resultado de 'chrem':`);
chrem([49,-21,-30],[99,97,95]);

print(`f \342\211\241 -5 mod 11\134nf \342\211\241 -2 mod 13\134nf \342\211\241 5 mod 17\134nf \342\211\241 9 mod 19`);
print(`Resultado implementaci\303\263n propia:`);
chinoRestos([-5,-2,5,9],[11,13,17,19]); 
print(`Resultado de 'chrem':`);
chrem([-5,-2,5,9],[11,13,17,19]); 

print(`Ejemplo en que los valores introducidos no son todos coprimos:`);
chinoRestos([-5,-2,5,9],[11,13,17,22]);
chrem([-5,-2,5,9],[11,13,17,22]);</EC-CodeEditor>
