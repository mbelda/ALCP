g := (x^2 + 1)*(x - 3):
mcd := mcdDFU(f, g);

print(`En Z: mcd(2x - 2, 2)`);
f := 2*x - 2:
g := 2:
mcd := mcdDFU(f, g);


#mcd en Zp DFU
mcdDFU := proc (f::polynom, g::polynom, p::integer)
	local r, n, i, auxr;
	if not isprime(p) then error &quot;p no es primo&quot; end if;
	r := [f mod p, g mod p];
	n := [degree(f), degree(g)];
	i := 2;
	while r[i] &lt;&gt; 0 do
		auxr := primpart(rem((lcoeff(r[i])^(n[i-1] -n[i] + 1)  * r[i-1]) mod p, r[i], x) mod p, x) mod p;
		r := [op(r), auxr];
		n := [op(n), degree(auxr)];
		i := i + 1;
	end do;
	return normal(r[i-1])
end proc:

