end proc:

edf := proc(f::polynom, p::integer, k::integer, d::integer)
	local g, result;
	if degree(f) &lt;= d then return [f] end if;

	g := edfAux(f, p, k, d);
	while g = -1 do
		g := edfAux(f, p, k, d);
	end do;

	result := [op(edf(g, p, k, d)), op(edf(quo(f, g, x) mod p, p, k, d))];
	
	return result
end proc:


polyFact := proc(f::polynom, p::integer, expo::integer)
	local U, h, i, q, v, facg, j, e, g;
	q := p^expo;
	v := f/lcoeff(f) mod p;
	U := [];
	h := x;
	i := 1;

	while v &lt;&gt; 1 do
		h := Powmod(h, q, v, x) mod p;
	   	g := Gcd(h-x, v) mod p;
	   	if g &lt;&gt; 1 then 
			facg := edf(g, p, expo, i);
			for j to numelems(facg) do
				e := 0;
				while Rem(v, facg[j], x) mod p = 0 do
					v := Quo(v, facg[j], x) mod p;
					e := e+1;
				end do;
				U := [op(U), [facg[j],e]];
			end do;
	   	end if;
	   	i := i+1;
	end do;
	return U
end proc:

