mcdDFU := proc (f::polynom, g::polynom)
	local r, n, i, auxr;
	r := [f, g];
	n := [degree(f), degree(g)];
	i := 2;
	while r[i] &lt;&gt; 0 do
		auxr := primpart(rem(lcoeff(r[i])^(n[i-1] -n[i] + 1) * r[i-1], r[i], x), x);
		r := [op(r), auxr];
		n := [op(n), degree(auxr)];
		i := i + 1;
	end do;
	return r[i-1]
end proc:


