</Group></Presentation-Block><Presentation-Block><CodeEditor-ExecGroup view="presentation" hide-input="false" hide-output="true" inline-output="false" labelreference="L2" display="code"><EC-CodeEditor id="CodeEditRegion0" visible="true" pixel-width="538" pixel-height="237" code-language="text/maple" autoexpand="false" wrapping="false" code-line-numbers="true">#Comprobaci\303\263n de que todos los enteros de una lista son primos entre s\303\255
areCoprime:=proc(e::list(integer))
	local i,j;
	for i from 1 to nops(e) do
		for j from 1 to nops(e) do
			if i &lt;&gt; j and igcd(e[i],e[j]) &lt;&gt; 1 then
				return false;
			fi;
		od;
	od;
	return true;
end proc:

# Teorema chino de los restos para Z
chinoRestos := proc(v::list(integer),m::list(integer)) 
	local prod_m, c, sol, i, s, t;
	if not areCoprimeZ(m) then
		return &quot;Error: los n\303\272meros introducidos han de ser coprimos&quot;;
	fi;
	prod_m := mul(m[i], i=1..nops(m));
	sol := 0; 
	c := 0;
	for i from 1 to nops(v) do
		s:='s';
		t:='t';
		igcdex(prod_m/(m[i]), m[i], s, t);
		c := irem(v[i]*s,m[i]);
		sol := sol + (c*prod_m)/(m[i]);
	od;  
	return sol mod prod_m;
end proc:

print(`Ejemplos del Teorema Chino de los Restos en Z:`);

print(`f \342\211\241 2 mod 11\134nf \342\211\241 7 mod 13`);
print(`Resultado implementaci\303\263n propia:`);
chinoRestos([2,7],[11,13]); 
print(`Resultado de 'chrem':`);
chrem([2,7],[11,13]);

print(`f \342\211\241 49 mod 99\134nf \342\211\241 -21 mod 97\134nf \342\211\241 -30 mod 95`);
