</Group></Presentation-Block><Presentation-Block><CodeEditor-ExecGroup view="presentation" hide-input="false" hide-output="true" inline-output="false" labelreference="L13" display="code"><EC-CodeEditor id="CodeEditRegion9" visible="true" pixel-width="552" pixel-height="555" code-language="text/maple" autoexpand="true" wrapping="false" code-line-numbers="true">sfd := proc(f::polynom, p::integer)
	local L, g, j, df, w, waux, pol, s;
	pol := f;
	L := [];
	s := 1;
	while pol &lt;&gt; 1 do
		j := 1;
		g := Quo(pol, Gcd(pol, diff(pol, x)) mod p, x) mod p;
		while g &lt;&gt; 1 do
			pol := Quo(pol, g, x) mod p;
			w := Gcd(pol, g) mod p;
			waux := Quo(g, w, x) mod p;
			if waux &lt;&gt; 1 then L := [op(L), [waux, j*s]]; end if;
			g := w;
			j := j+1;
		end do;
		if f &lt;&gt; 1 then
			pol := proot(pol, p);
			s := p*s;
		end if;
	end do;
	return L
end proc:

