</Group></Presentation-Block><Presentation-Block><CodeEditor-ExecGroup view="presentation" hide-input="false" hide-output="true" inline-output="false" display="code"><EC-CodeEditor id="CodeEditRegion16" visible="true" pixel-width="538" pixel-height="646" code-language="text/maple" autoexpand="false" wrapping="false" code-line-numbers="true">restart;
MRTest := proc(n::integer, k::integer)
	local s,q, a,b,i,j,seguir;
	q:= n - 1;
	s:= 0;
	while q mod 2 = 0 do
		s:= s+ 1;
		q:= iquo(q,2);
	od;
	
	for i from 1 to k do
		seguir:=0;
		a:= rand(2..n-2)();
		b:= a^q mod n;
		if b = 1 or b = (n-1) then
			seguir:=1;
		fi;
		if seguir = 0 then
			for j from 1 to s-1 do
				b := b^2 mod n;
				if b = n-1 then
					seguir:=1;
					break;
				fi;
				if b = 1 then
					return &quot;composite&quot;;
				fi;
			od;
			if seguir = 0 then
				return &quot;composite&quot;;
			fi;
		fi;
	od;
	return &quot;probably prime&quot;;

end proc:

# Veinte primos
print(`Primos:`);
for i from 1 to 5 do
	n := ithprime(rand(60..900)());
	MRTest(n,15);
	isprime(n); 
od;

# Veinte n\303\272meros que no son primos
print(`No primos:`);
for i from 1 to 5 do
	n:=rand(50..100)()*rand(50..100)();
	MRTest(n,15); 
	isprime(n);
od;</EC-CodeEditor>
