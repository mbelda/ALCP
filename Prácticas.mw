aks := proc(n::posint)
	local r, orden, a, i,fl, cmp;
	if iperfpow(n) &lt;&gt; FAIL then
		return &quot;composite&quot;
	fi;

	r:=1;
	orden:=0;
	while(orden &lt; round(log[2](n)^2)) do
		r:=r+1;
		orden:=order(n,r);
		if(orden=FAIL) then
			orden:=0;
		fi;
	od;

	for a from 2 to min(r,n-1) do
		if igcd(a,n)&lt;&gt;1 then
			return &quot;composite&quot;;
		fi;
	od;

	if n &lt;= r then
		return &quot;prime&quot;;
	fi;

	fl:=floor(sqrt(phi(r))*log[2](n));
	for i from 1 to fl do
		if Powmod(x+i,n,x^r-1,x) mod n &lt;&gt; Rem(x^n+i,x^r-1,x) mod n then
			return &quot;composite&quot;;
		fi;
	od;

	return &quot;prime&quot;;
end proc:

# Veinte primos
print(`Primos:`);
for i from 1 to 5 do
	n := ithprime(rand(60..900)());
	isprime(n);
	aks(n); 
od;

# Veinte n\303\272meros que no son primos
print(`No primos:`);
for i from 1 to 5 do
	n:=rand(50..100)()*rand(50..100)();
	isprime(n); 
	aks(n);
od;</EC-CodeEditor>
