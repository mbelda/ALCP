</Group></Presentation-Block><Presentation-Block><CodeEditor-ExecGroup view="presentation" hide-input="false" hide-output="true" inline-output="false" labelreference="L17" display="code"><EC-CodeEditor id="CodeEditRegion11" visible="true" pixel-width="616" pixel-height="707" code-language="text/maple" autoexpand="true" wrapping="false" code-line-numbers="true">edfAux := proc(f::polynom, p::integer, k::integer, d::integer)
	local n, a, g1, b, g2, q;
	n := degree(f);
	q := p^k;
	
     a := Randpoly(rand(n)(), x) mod p;
	if degree(a) &lt; 1 then return -1 end if;
	
	g1 := Gcd(a, f) mod p;
	if g1 &lt;&gt; 1 then return g1 end if;
	
	b := Powmod(a, (q^d - 1)/2, f, x) mod p;
	
	g2 := Gcd(b-1, f) mod p;
	if g2 &lt;&gt; 1 and g2 &lt;&gt; f then return g2
	else return -1 end if;
end proc:

edf := proc(f::polynom, p::integer, k::integer, d::integer)
	local g, result;
	if degree(f) &lt;= d then return [f] end if;

	g := edfAux(f, p, k, d);
	while g = -1 do
		g := edfAux(f, p, k, d);
	end do;

	result := [op(edf(g, p, k, d)), op(edf(quo(f, g, x) mod p, p, k, d))];
	
	return result
end proc:

