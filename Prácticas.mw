</Group></Presentation-Block><Presentation-Block><CodeEditor-ExecGroup view="presentation" hide-input="false" hide-output="true" inline-output="false" labelreference="L5" display="code"><EC-CodeEditor id="CodeEditRegion7" visible="true" pixel-width="769" pixel-height="398" code-language="text/maple" autoexpand="false" wrapping="false" code-line-numbers="true">with(GaussInt, GIrem, GInormal, GIquo):

#Euclides para Z
euclidesExt := proc (f::integer, g::integer)
   local r0, r1, r2, s0, s1, s2, t0, t1, t2;
   r0, r1 := f, g;
   s0, s1 := 1, 0;
   t0, t1 := 0, 1;
   while r1 &lt;&gt; 0 do
       r2 := irem(r0, r1);
       s2 := s0 - s1*iquo(r0, r1);
       t2 := t0 - t1*iquo(r0, r1);
       r0, s0, t0 := r1, s1, t1;
       r1, s1, t1 := r2, s2, t2;
   end do;
   return r0, s0, t0
end proc:

print('________________EJEMPLOS________________');
print(`En Z: mcd(12,30)`);
f := 12:
g := 30:
mcd, u, v := euclidesExt(f,g);
evalb(mcd -u*f -v*g = 0);

print(`En Z: mcd(18,6)`);
f := 18:
g := 6:
mcd, u, v := euclidesExt(f,g);
evalb(mcd -u*f -v*g = 0);

#Euclides para Zp
euclidesExtzp := proc (f::integer, g::integer, p::integer)
   local r0, r1, r2, s0, s1, s2, t0, t1, t2;
   if not isprime(p) then error &quot;p no es primo&quot; end if;
   r0, r1 := f mod p, g mod p;
   s0, s1 := 1, 0;
   t0, t1 := 0, 1;
   while r1 &lt;&gt; 0 do
       r2 := irem(r0, r1) mod p;
       s2 := s0 - s1*iquo(r0, r1);
       t2 := t0 - t1*iquo(r0, r1);
       r0, s0, t0 := r1, s1, t1;
       r1, s1, t1 := r2, s2, t2;
   end do;
   return r0, s0, t0
end proc:

print('________________________________________');
print(`En Z7: mcd(-3,4)`);
f := -3:
g := 4:
p := 7:
mcd, u, v := euclidesExtzp(f, g, p);
evalb((mcd -u*f -v*g) mod p = 0);

print(`En Z5: mcd(2,12)`);
f := 2:
g := 12:
p := 5:
mcd, u, v := euclidesExtzp(f, g, p);
evalb((mcd -u*f -v*g) mod p = 0);

print(`En Z5: mcd(2,3)`);
f := 2:
g := 3:
p := 5:
mcd, u, v := euclidesExtzp(f, g, p);
evalb((mcd -u*f -v*g) mod p = 0);

#Euclides para Z[i]
euclidesiExt := proc (f::complex(integer), g::complex(integer))
   local r0, r1, r2, s0, s1, s2, t0, t1, t2;
   r0, r1 := f, g;
   s0, s1 := 1, 0;
   t0, t1 := 0, 1;
   while r1 &lt;&gt; 0 do
       r2 := GIrem(r0, r1);
       s2 := s0 - s1*GIquo(r0, r1);
       t2 := t0 - t1*GIquo(r0, r1);
       r0, s0, t0 := r1, s1, t1;
       r1, s1, t1 := r2, s2, t2;
   end do;
   return r0, s0, t0
end proc:

print('________________________________________');
print(`En Z[i]: mcd(-7 + 11I, -12 + 11I)`);
f := Complex(-7, 11):
g := Complex(-12, 11):
mcd, u, v := euclidesiExt(f,g);
evalb(mcd -u*f -v*g = 0);

print(`En Z[i]: mcd(I + 6, (I + 6)^2)`);
f := 6 + I;
g := (g + I)^2:
mcd, u, v := euclidesiExt(6 + I, (6 + I)^2);
evalb(mcd -u*f -v*g = 0);


#Euclides para Q[x]
euclidesExtqx := proc (f::polynom, g::polynom)
   local r0, r1, r2, s0, s1, s2, t0, t1, t2;
   r0, r1 := f, g;
   s0, s1 := 1, 0;
   t0, t1 := 0, 1;
   while r1 &lt;&gt; 0 do
       r2 := rem(r0, r1, x);
       s2 := s0 - s1*quo(r0, r1, x);
       t2 := t0 - t1*quo(r0, r1, x);
       r0, s0, t0 := r1, s1, t1;
       r1, s1, t1 := r2, s2, t2;
   end do;
   return r0, s0, t0
end proc:

print('________________________________________');
print(`En Q[x]: mcd(x^3 + 2*x^2 - x - 2, x^3 + 4*x^2 + x - 6)`);
f := x^3 + 2*x^2 - x - 2:
g := x^3 + 4*x^2 + x - 6:
mcd, u, v := euclidesExtqx(f,g);
evalb(mcd -u*f -v*g = 0);

print(`En Q[x]: mcd((2x+1)^3, 2x+1)`);
f := (2*x+1)^3:
g := 2*x+1:
mcd, u, v := euclidesExtqx(f,g);
evalb(mcd -u*f -v*g = 0);

#Euclides para Fq[x]
euclidesExtfpx := proc (f::polynom, g::polynom, p::integer)
   local r0, r1, r2, s0, s1, s2, t0, t1, t2;
   if not isprime(p) then error &quot;p no es primo&quot; end if;
   r0, r1 := f, g;
   s0, s1 := 1, 0;
   t0, t1 := 0, 1;
   while r1 &lt;&gt; 0 do
       r2 := Rem(r0, r1, x) mod p;
       s2 := s0 - s1*quo(r0, r1, x);
       t2 := t0 - t1*quo(r0, r1, x);
       r0, s0, t0 := r1, s1, t1;
       r1, s1, t1 := r2, s2, t2;
   end do;
   return r0 mod p, s0 mod p, t0 mod p
end proc:

print('________________________________________');
print(`En F7[x]: mcd(rx^2 - r, rx + r)`);
f := r*x^2 - r:
g := r*x + r:
p := 7:
mcd, u, v := euclidesExtfpx(f,g,p);
evalb((mcd -u*f -v*g) mod p = 0);</EC-CodeEditor>
