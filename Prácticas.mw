with(LinearAlgebra):
with(PolynomialTools):
berlekamp := proc(p::integer,k::integer,f::polynom) # El polinomio ha de ser libre de cuadrados
	local n, qi, xq,Q,Id,U,B,r,a,g1,g2,b,i,j,l,qiV,M,pol,q;
	n:=degree(f,x);
	if coeff(f,x,n) &lt;&gt; 1 then
		return &quot;Error: el polinomio ha de ser m\303\263nico.&quot;
	fi;
	q:=p^k;
	xq:=Powmod(x,q,f,x) mod p;
	Q := Matrix(n);
	for i from 0 to n - 1 do
		qi := Powmod(xq,i,f,x) mod p;
		qiV := CoefficientVector(qi,x);
		for j from 1 to numelems(qiV) do
			Q[i+1,j]:=qiV[j];
		od:
	od:
	Id:=Matrix(n,shape=identity);
	M:=Q-Id;
	U:=Gausselim(M) mod p;
	B:=NullSpace(U) mod p;
	r:=numelems(B);
	if r = 1 then
		return f;
	fi:
	a:= 0;
	for l from 1 to r do
		a:= a + FromCoefficientVector(B[l],x)*rand(q)();
	od:
	g1:=Gcd(a,f) mod p;
	if g1&lt;&gt;1 and g1&lt;&gt;f then
		return g1;
	fi:
	b:=Powmod(a,iquo(q-1, 2),f,x) mod p;
	g2:=Gcd(b-1,f) mod p;
	if g2&lt;&gt;1 and g2&lt;&gt;f then
		return g2;
	fi:
		return &quot;failure&quot;;
	
end proc:

print(`Ejemplo en F5[x]: x^9 + x`);
Factor(x^9+x) mod 5;
for i from 1 to 10 do
	berlekamp(5, 1,x^9+x);
od;

print(`\134nEjemplo en F4[x]: x^2 + x + 1`);
alias(alpha = RootOf(x^2 + x + 1)):
Factor(x^5+alpha, alpha) mod 2;
for i from 1 to 10 do
	berlekamp(2, 2,x^5+alpha);
od;</EC-CodeEditor>
