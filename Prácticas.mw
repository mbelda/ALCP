disc := proc(g,p::integer,k::integer,h)
	local m, alpha, a, b,i,j,q,g2,h2;
	g2:=evala(g) mod p;
	h2:=evala(h) mod p;
	q:=p^k;
	m := ceil(sqrt(q));
	alpha:=[];
	alpha:=[op(alpha),1];
	for i from 1 to m-1 do
		alpha:= [op(alpha),evala (alpha[-1]*g2) mod p];
	od; 
	a:= evala(g2^(-m)) mod p;
	b:=h2;
	for j from 0 to m-1 do
		if member(b,alpha,'r') then
			return j*m+(r-1);
		fi;
		b:= evala(b*a) mod p;
	od;
	return &quot;sin soluci\303\263n&quot;;

end proc:

