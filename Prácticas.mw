#Euclides para Z
euclides := proc (f::integer, g::integer)
   local r0, r1, r2;
   r0, r1 := abs(f), abs(g);
   while r1 &lt;&gt; 0 do
       r2 := irem(r0, r1);
       r0 := r1;
       r1 := r2;
   end do;
   return r0
end proc:

print('________________EJEMPLOS________________');
print(`En Z: mcd(12,32)`);
euclides(32, 12);
print(`En Z: mcd(5,7)`);
euclides(5, 7);

#Euclides para Zp
euclides := proc (f::integer, g::integer, p::integer)
   local r0, r1, r2;
   if not isprime(p) then error &quot;p no es primo&quot; end if;
   r0, r1 := f, g;
   while r1 &lt;&gt; 0 do
       r2 := irem(r0, r1) mod p;
       r0 := r1;
       r1 := r2;
   end do;
   return r0
end proc:

print('________________________________________');
print(`En Z7: mcd(-3,4)`);
euclides(-3, 4, 7);
print(`En Z5: mcd(2,12)`);
euclides(2, 12, 5);



#Euclides para Z[i]
euclidesi := proc (f::complex(integer), g::complex(integer))
   local r0, r1, r2;
   r0, r1 := f, g;
   while r1 &lt;&gt; 0 do
       r2 := GIrem(r0, r1);
       r0 := r1;
       r1 := r2;
   end do;
   return GInormal(r0)
end proc:

print('________________________________________');
print(`En Z[i]: mcd(I + 6, (I + 6)^2)`);
euclidesi(6 + I, (6 + I)^2);
print(`En Z[i]: mcd(3, 3I)`);
euclidesi(3, 3*I);
print(`En Z[i]: mcd(2, 3I)`);
euclidesi(2, 3*I);

#Euclides para Q[x]
euclidesqx := proc (f::polynom, g::polynom)
   local r0, r1, r2;
   r0, r1 := f, g;
   while r1 &lt;&gt; 0 do
       r2 := rem(r0, r1, x);
       if r2 &lt;&gt; 0 then r2 := r2/lcoeff(r2); end if;
       r0 := r1;
       r1 := r2;
   end do;
   return r0
end proc:

print('________________________________________');
print(`En Q[x]: mcd(x+1, (x+1)(x-1))`);
euclidesqx(x + 1, (x+1)*(x-1));
print(`En Q[x]: mcd(x-3, (x+1)(x-3))`);
euclidesqx(x-3, (x+1)*(x-3));
print(`En Q[x]: mcd(x-1, (x+1)(x-3))`);
euclidesqx(x-1, (x+1)*(x-3));


#Euclides para Fq[x]
euclidesfpx := proc (f::polynom, g::polynom, p::integer)
   local r0, r1, r2;
   if not isprime(p) then error &quot;p no es primo&quot; end if;
   r0, r1 := f mod p, g mod p;
   while r1 &lt;&gt; 0 do
       r2 := Rem(r0, r1, x) mod p;
       r0 := r1;
       r1 := r2;
   end do;
   return r0
end proc:

print('________________________________________');
print(`En F7[x]: mcd(rx^2 - r, rx + r)`);
euclidesfpx(r*x^2 - r, r + x*r, 7);</EC-CodeEditor>
