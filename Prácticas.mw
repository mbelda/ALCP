</Group></Presentation-Block><Presentation-Block><CodeEditor-ExecGroup view="presentation" hide-input="false" hide-output="true" inline-output="false" labelreference="L23" display="code"><EC-CodeEditor id="CodeEditRegion14" visible="true" pixel-width="801" pixel-height="833" code-language="text/maple" autoexpand="false" wrapping="false" code-line-numbers="true">edfAux := proc(f::polynom, p::integer, k::integer, d::integer)
	local n, a, g1, b, g2, q;
	n := degree(f);
	q := p^k;
	
     a := Randpoly(rand(n)(), x) mod p;
	if degree(a) &lt; 1 then return -1 end if;
	
	g1 := Gcd(a, f) mod p;
	if g1 &lt;&gt; 1 then return g1 end if;
	
	b := Powmod(a, (q^d - 1)/2, f, x) mod p;
	
	g2 := Gcd(b-1, f) mod p;
	if g2 &lt;&gt; 1 and g2 &lt;&gt; f then return g2
	else return -1 end if;
end proc:

edf := proc(f::polynom, p::integer, k::integer, d::integer)
	local g, result;
	if degree(f) &lt;= d then return [f] end if;

	g := edfAux(f, p, k, d);
	while g = -1 do
		g := edfAux(f, p, k, d);
	end do;

	result := [op(edf(g, p, k, d)), op(edf(quo(f, g, x) mod p, p, k, d))];
	
	return result
end proc:


polyFact := proc(f::polynom, p::integer, expo::integer)
	local U, h, i, q, v, facg, j, e, g;
	q := p^expo;
	v := f/lcoeff(f) mod p;
	U := [];
	h := x;
	i := 1;

	while v &lt;&gt; 1 do
		h := Powmod(h, q, v, x) mod p;
	   	g := Gcd(h-x, v) mod p;
	   	if g &lt;&gt; 1 then 
			facg := edf(g, p, expo, i);
			for j to numelems(facg) do
				e := 0;
				while Rem(v, facg[j], x) mod p = 0 do
					v := Quo(v, facg[j], x) mod p;
					e := e+1;
				end do;
				U := [op(U), [facg[j],e]];
			end do;
	   	end if;
	   	i := i+1;
	end do;
	return U
end proc:

#expande la lista que le pasamos
auxModFact := proc(l)
	local lista, i, j;
	lista := [];
	for i in l do
		for j to i[2] do
			lista := [op(lista), i[1]];
		end do;
	end do;
end proc:


#Calcula el productorio con indices
prodIndices := proc(l, Saux)
	local i, g;
	g := 1;
	for i in Saux do
		g := g*l[i];
	end do;
	return g
end proc:


factZx := proc(f::polynom)
	local n, A, b, B, p, fbar, modFact, T, s, G, fest, S, gest, hest, Saux;
	n := degree(f);
	A := maxnorm(f);
	
	#1
	if n = 1 then return f end if;
	b := lcoeff(f);
	B := (n+1)^(1/2)*2^n*A*b;

	#2
	p := nextprime(floor(2*B));
	fbar := f mod p;
	while Gcd(fbar, diff(fbar, x)) mod p &lt;&gt; 1 do
		p := nextprime(floor(p));
		fbar := f mod p;
	end do;

	#3: Factorizaci\303\263n modular
	modFact := auxModFact(polyFact(f, p, 1));

	#5 Inicializar
	T := {seq(i, i=1 ..nops(modFact))};
	s := 1;
	G := {}; #Factores encontrados
	fest := f;

	#6 Combinaci\303\263n de factores
	while 2*s &lt;= nops(T) do
		bucle:
		#7 Para todos los ScT con #S=s
		S := combinat[choose](T, s);
		for Saux in S do
			#8 Calcular g*, h*
			gest := b*expand(prodIndices(modFact, Saux)) mod p;
			hest := b*expand(prodIndices(modFact, T minus Saux)) mod p;
			#9
			if norm(gest,1)*norm(hest,1) &lt;= evalf(B) then
				T := T minus Saux;
				G := G union {primpart(gest)};
				fest := primpart(hest);
				b := lcoeff(fest);
				break;
				goto(bucle)
			end if;
		end do;
		#10
		s := s+1;
	end do;
	return G union {fest}
end proc:

print('________________EJEMPLOS________________');
print(`En Z5^4: x^4 + 3x^3 + 4x^2 + 3`);
factZx(6*x^4 + 5*x^3 + 15*x^2 + 5*x + 4);</EC-CodeEditor>
